   "source": [
    "def N_iterative(N_0, p, number_iterations):\n",
    "N_values=[N_0]\n",
    "current_N=N_0\n",
    "for _ in range(number_iterations):\n",
    "    current_N = current_N + p\n",
    "    N_values.append(current_N)\n",
    "    return N_values\n",
    "    \n",
    "    \n"
   ]
